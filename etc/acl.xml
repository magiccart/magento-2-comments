<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
	    <resources>
	        <resource id="Magento_Backend::admin">
	        	<resource id="Magiccart_Comments::magiccart" title="Magiccart" sortOrder="10">
	        	    <resource id="Magiccart_Comments::comments" title="Comments" sortOrder="10">
	        	        <resource id="Magiccart_Comments::comments_index" title="Comments" sortOrder="10" />
	        	    </resource>
	        	</resource>
	            <resource id="Magento_Backend::stores">
	                <resource id="Magento_Backend::stores_settings">
	                    <resource id="Magento_Config::config">
	                        <resource id="Magiccart_Comments::config_comments" title="Comments" />
	                    </resource>
	                </resource>
	            </resource>
	        </resource>
	    </resources>
	</acl>
</config>
